@{
    ViewBag.Title = "Reservar - FullDay exitosa";

}

    <body>
        <div class="container center-block col-md-offset-2">
            @if (ViewBag.Pk_Reserva == 0)
            {
                <p style="color:red; font-size:14px;">No se pudo realizar la reserva</p>
            }
            else
            {

                <p style="color:limegreen; font-size:16px;">Exito al generar la reserva N° @ViewBag.Pk_Reserva  </p>
                <p style="font-size:14px;" >Nuestros operadores se van a estar comunicando dentro de las 24 horas con usted para realizar el pago correspondiente de la reserva </p>
            }
            <div class="row">
                <div class="col-md-offset-4 col-md-2">
                    @Html.ActionLink("FullDay", "FullDay", "Proyecto", null, new { @class = "btn btn-info" })
                </div>
            </div>
          

        </div>
       

    </body>

<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

<script src="~/Scripts/jquery-3.4.1.min.js"></script>
